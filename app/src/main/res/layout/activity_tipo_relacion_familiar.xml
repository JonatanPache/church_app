<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".View.TipoRelacionFamiliarActivity">

    <TextView
        android:id="@+id/labNameTipoRelacionFamiliar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre:" />

    <EditText
        android:id="@+id/txtNombreTipoRelacionFamiliar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="88dp" />

    <Button
        android:id="@+id/btnSaveTipoRelacionFamiliar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Guardar Tipo Relacion"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="341dp" />
</LinearLayout>